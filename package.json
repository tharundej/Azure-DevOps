{
  "name": "Nextzen",
  "author": "Infobell It",
  "version": "0.0.1",
  "description": "Next.Zen TO SPACE",
  "private": true,
  "proxy": "https://api-dev-minimal-v510.vercel.app",
  "scripts": {
    "start:dev": "set PORT=3000 && env-cmd -f .env.dev react-scripts --max_old_space_size=4096 start",
    "start:stage": "set PORT=3000 && env-cmd -f .env.stage react-scripts --max_old_space_size=4096 start",
    "start:prod": "set PORT=3000 && env-cmd -f .env.prod react-scripts --max_old_space_size=4096 start",
    "build:dev": "env-cmd -f .env.dev react-scripts --max_old_space_size=4096 build",
    "build:stage": "env-cmd -f .env.stage react-scripts --max_old_space_size=4096 build",
    "build:prod": "env-cmd -f .env.build react-scripts --max_old_space_size=4096 build",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "clear-all": "rm -rf node_modules .next out dist build",
    "re-start": "rm -rf node_modules .next out dist build && yarn install && yarn start",
    "re-build": "rm -rf node_modules .next out dist build && yarn install && yarn build",
    "re-build-test": "rm -rf node_modules .next out dist build && npm install && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
